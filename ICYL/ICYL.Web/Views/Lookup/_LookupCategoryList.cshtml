@model ICYL.BL.LookupValueBLList
@using ICYL.BL
@using EnterpriseLayer.Utilities;
<div class="row">
    <div class="col-lg-12">
        <div>
            <div class="ibox-content">
                <div class="row">
                    <div>
                        <table id="tblLookupCategoryReport" class="table table-striped table-bordered table-hover dataTable">
                            <thead>
                                <tr>
                                    <th class="hidden"></th>
                                    <th class="hidden"></th>
                                    <th>Value</th>
                                    <th>Description</th>
                                    <th>API Login ID</th>
                                    <th>Transaction Key</th>
                                    <th>Display Order</th>
                                    <th></th>
                                </tr>
                            </thead>
                            <tbody>
                                @{
                                    if (Model != null)
                                    {
                                        if (Model.lookupValuelst != null)
                                        {
                                            foreach (var item in Model.lookupValuelst)
                                            {
                                                                <tr>
                                                                    <td class="left hidden ValueId"><span>@MvcHtmlString.Create(Conversion.ConversionToString(item.ValueId))</span></td>
                                                                    <td class="left hidden GroupId"><span>@MvcHtmlString.Create(Conversion.ConversionToString(item.GroupId))</span></td>
                                                                    <td class="left Value">
                                                                        <span>@MvcHtmlString.Create(Conversion.ConversionToString(item.Value))</span>
                                                                        <input type="text" value="@item.Value" style="display:none" class="form-control" />
                                                                    </td>
                                                                    <td class="left ValueDescription">
                                                                        <span>@MvcHtmlString.Create(Conversion.ConversionToString(item.ValueDescription))</span>
                                                                        <input type="text" value="@item.ValueDescription" style="display:none" class="form-control required" />
                                                                    </td>
                                                                    <td class="left APIId">
                                                                        <span>@MvcHtmlString.Create(Conversion.ConversionToString(item.APIId))</span>
                                                                        <input type="text" value="@item.APIId" style="display:none" class="form-control required" />
                                                                    </td>
                                                                    <td class="left APIKey">
                                                                        <span>@MvcHtmlString.Create(Conversion.ConversionToString(item.APIKey))</span>
                                                                        <input type="text" value="@item.APIKey" style="display:none" class="form-control required" />
                                                                    </td>
                                                                    <td style="width:8%" class="left DisplayOrder">
                                                                        <span>@MvcHtmlString.Create(Conversion.ConversionToString(item.DisplayOrder))</span>
                                                                        <input type="text" value="@item.DisplayOrder" class="form-control" style="display:none" maxlength="5" />
                                                                    </td>
                                                                    <td style="width:10%">
                                                                        <a class="Edit" href="javascript:;"><i class="fa fa-edit"></i>Edit</a>&nbsp;&nbsp;
                                                                        <a class="Update" href="javascript:;" style="display:none"><i class="fa fa-edit"></i>Update</a>&nbsp;&nbsp;
                                                                        <a class="Cancel" href="javascript:;" style="display:none"><i class="fa fa-circle-o"></i>Cancel</a>&nbsp;&nbsp;
                                                                        <a class="Delete" onclick="DeleteLookup('@item.ValueId','@item.Active.ToString()');" href="javascript:;"><i class="fa fa-times-circle-o"></i>@Html.Conditional(item.Active, "Disable", "ENABLE")</a>
                                                                    </td>
                                                                </tr>
                                            }
                                        }
                                    }
                                }
                            </tbody>
                        </table>
                    </div>

                </div>
            </div>
        </div>
    </div>
</div>
